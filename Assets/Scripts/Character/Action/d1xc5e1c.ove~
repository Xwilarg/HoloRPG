using HoloRPG.SO;
using System.Linq;

namespace HoloRPG.Character.Action
{
    public class ActionPlanner
    {
        public ActionPlanner()
        {
            _actions = Enumerable.Repeat(ActionType.NONE, StaticResources.S.GameInfo.DefaultTurnCount);
        }

        private ActionType[] _actions;
    }
}
